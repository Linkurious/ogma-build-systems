{
  "name": "@linkurious/ogma-build-systems",
  "version": "0.0.1",
  "description": "Build system templates for Ogma.",
  "main": "index.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "concurrently \"npm run build:vite\" \"npm run build:rollup\" \"npm run build:webpack\" \"npm run build:typescript\"",
    "build:vite": "cd vite && npm run build",
    "build:rollup": "cd rollup && npm run build",
    "build:webpack": "cd webpack && npm run build",
    "build:typescript": "cd typescript && npm run build",
    "build:node": "cd node && npm run build",
    "prepare": "node scripts/prepare.js && concurrently \"npm run prepare:vite\" \"npm run prepare:rollup\" \"npm run prepare:webpack\" \"npm run prepare:typescript\" \"npm run prepare:node\" && node scripts/cleanup.js",
    "prepare:vite": "cd vite && npm i",
    "prepare:rollup": "cd rollup && npm i",
    "prepare:webpack": "cd webpack && npm i",
    "prepare:typescript": "cd typescript && npm i",
    "prepare:node": "cd node && npm i",
    "postversion": "sync_versions"
  },
  "keywords": [
    "ogma",
    "webpack",
    "vite",
    "template"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@linkurious/code-tools": "^0.0.14",
    "concurrently": "^7.6.0"
  },
  "dependencies": {
    "@linkurious/ogma": "4.4.3-develop.4"
  }
}
